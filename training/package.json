{
  "name": "training",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "commitizen": "^3.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-scripts": "2.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:c": "npm run test -- --coverage",
    "eject": "react-scripts eject",
    "lint": "npm run start ",
    "commit": "npx git-cz",
    "lint:fix": "eslint --fix --ext .js,.vue src"
  },
  "hooks": {
    "pre-commit": "npm run lint && npm run test:c",
    "pre-push": "npm run lint && npm run test:c"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "husky": "^1.3.1",
    "i": "^0.3.6",
    "npm": "^6.8.0"
  }
}
